// Comprehensive pattern matching test
func testPatterns(value: i32) str {
    if(value > 0) return "Go fuck yourself";
    return value => {
        0 -> "Zero",
        1 -> "One", 
        2 to 10 -> "Small",
        _ -> "Large"
    };
}

func main() i32 {
    def result = testPatterns(5);
    return 0;
}
