// Pattern matching with guard conditions

func testGuards(x: i32) str {
    return x => {
        n && n > 0 -> "positive",
        n && n < 0 -> "negative", 
        0 -> "zero",
        _ -> "unknown"
    };
}

func testRangeWithGuards(x: i32) str {
    return x => {
        1 to 10 && x % 2 == 0 -> "even in range",
        1 to 10 -> "odd in range",
        n && n > 100 -> "large number",
        _ -> "other"
    };
}

func main() i32 {
    return 0;
}
