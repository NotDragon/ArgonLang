// Error handling and result types (simplified)
func safeDivide(a: i32, b: i32) i32 {
    if (b == 0) {
        return -1; // Error indicator
    }
    return a / b;
}

func processResult(value: i32) str {
    if (value < 0) {
        return "Error occurred";
    }
    return "Success: " + value;
}

func main() i32 {
    def result1: i32 = safeDivide(10, 2);
    def result2: i32 = safeDivide(10, 0);
    
    def message1: str = processResult(result1);
    def message2: str = processResult(result2);
    
    return 0;
}
