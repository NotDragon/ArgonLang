// Comprehensive range test
func useRanges() i32 {
    // Basic ranges
    def numbers = 1 to 5;           // [1, 2, 3, 4]
    def inclusive = 1 to= 5;        // [1, 2, 3, 4, 5]
    
    // Variable bounds
    def start: i32 = 10;
    def end: i32 = 20;
    def varRange = start to end;    // [10, 11, ..., 19]
    def varInclusive = start to= end; // [10, 11, ..., 20]
    
    return 42;
}

func main() i32 {
    return useRanges();
}
