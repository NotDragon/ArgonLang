// Simple advanced pattern matching tests

func testWildcard(x: i32) str {
    return x => {
        _ -> "anything"
    };
}

func testRangeInclusive(x: i32) str {
    return x => {
        1 to= 5 -> "inclusive range",
        _ -> "outside"
    };
}

func testMultipleLiterals(x: i32) str {
    return x => {
        42 -> "answer",
        100 -> "century",
        0 -> "zero",
        _ -> "other"
    };
}

func main() i32 {
    return 0;
}
