// Range expressions test
func testRanges() i32 {
    def range1 = 0 to 10;        // exclusive: [0, 1, 2, ..., 9]
    def range2 = 0 to= 10;       // inclusive: [0, 1, 2, ..., 10]
    def range3 = 5 to 15;        // exclusive: [5, 6, 7, ..., 14]
    
    return 0;
}

func main() i32 {
    def result: i32 = testRanges();
    return result;
}
